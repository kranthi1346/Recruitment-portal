    "react-ga": "^3.3.0",
    "react-gtm-module": "^2.0.11",
    "react-number-format": "^4.8.0",
    "react-observer-api": "^1.0.12",
    "react-redux": "^7.2.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-use-hotjar": "^1.2.1",
    "redux": "^4.0.4",
