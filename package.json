{
  "name": "recruitment-tool-frontend-v1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start:staging": "NODE_ENV=staging && next dev",
    "start:preprod": "NODE_ENV=preprod && next dev",
    "start:prod": "NODE_ENV=production && next dev",
    "build": "next build",
    "build:staging": "set NODE_ENV=staging && next build",
    "build:preprod": "set NODE_ENV=preprod && next build",
    "build:prod": "set NODE_ENV=production && next build",
    "start": "NODE_ENV=development npm start",
    "lint": "next lint,eslint --ext .js,.jsx,.ts,.tsx src --cache --cache-location ./.eslintlocalcache",
    "lint-fix": "next lint,eslint --ext .js,.jsx,.ts,.tsx src --cache --cache-location ./.eslintlocalcache",
    "lint-reset-cache": "rm -f ./.eslintlocalcache",
    "eject": "react-scripts eject",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ],
    "*.{jsx,js,tsx,ts}": [
      "eslint --cache --cache-location ./.eslintlocalcache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint",
      "pre-rebase": "npm  run lint-reset-cache",
      "post-checkout": "npm run lint-reset-cache"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@date-io/moment": "^2.11.0",
    "@devexpress/dx-react-chart": "^3.0.2",
    "@devexpress/dx-react-chart-material-ui": "^3.0.2",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@microsoft/applicationinsights-react-js": "^17.0.0",
    "@microsoft/applicationinsights-web": "^3.0.2",
    "@mui/material": "^5.15.15",
    "axios": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "chart.js": "^3.7.1",
    "chartjs-plugin-datalabels": "^2.0.0",
    "date-fns": "^2.28.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fg-loadcss": "^3.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "14.2.0",
    "next-redux-wrapper": "^4.0.1",
    "prettier-eslint": "^13.0.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^4.1.0",
    "react": "^18",
    "react-awesome-reveal": "^3.8.1",
    "react-awesome-slider": "^4.1.0",
    "react-chartjs-2": "^4.0.1",
    "react-countdown-now": "^2.1.2",
    "react-dom": "^18",
